<div class='container px-5 px-sm-0'>
  <div class='row'>
    <h4>会員一覧</h4>
    <table class='table'>
        <thead>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>会員ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @customers.each do |customer| %>
          <tr>
            <td><%= customer.id %></td>
            <td><%= link_to customer.last_name, edit_customer_registration_path %></td>
            <td><%= customer.email %></td>
            <td><%= customer.is_deleted %></td>
          </tr>
          <% end %>
        </tbody>
    </table>